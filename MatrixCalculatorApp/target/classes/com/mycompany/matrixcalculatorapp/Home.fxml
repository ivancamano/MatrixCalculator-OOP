<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.Lighting?>
<?import javafx.scene.effect.Shadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.matrixcalculatorapp.PrimaryController">
   <children>
      <AnchorPane prefHeight="416.0" prefWidth="600.0">
         <children>
            <ImageView fitHeight="416.0" fitWidth="600.0" pickOnBounds="true">
               <image>
                  <Image url="@../../../bg.jpg" />
               </image>
               <effect>
                  <Lighting diffuseConstant="0.68" specularConstant="0.92" specularExponent="40.0" surfaceScale="0.0">
                     <bumpInput>
                        <Shadow />
                     </bumpInput>
                     
                                    </Lighting>
               </effect>
            </ImageView>
            <Button fx:id="operationsButton" layoutX="226.0" layoutY="287.0" mnemonicParsing="false" onAction="#handleOperationsButtonClick" prefHeight="26.0" prefWidth="147.0" text="Matrix Operations" textFill="#0e1778">
               <font>
                  <Font name="Calisto MT" size="12.0" />
               </font>
            </Button>
            <Button fx:id="closeButton" layoutX="226.0" layoutY="325.0" mnemonicParsing="false" onAction="#handleCloseButtonClick" prefHeight="14.0" prefWidth="147.0" text="Close" textFill="#0e1778">
               <font>
                  <Font name="Calisto MT" size="12.0" />
               </font>
            </Button>
            <ImageView fx:id="logoImageView" fitHeight="206.0" fitWidth="318.0" layoutX="161.0" layoutY="50.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../logo.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>